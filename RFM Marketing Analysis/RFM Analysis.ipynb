   "source": [
    "def customerSegmentation(x):\n",
    "    if (x[0] == '1') & (x[1] == '1') & (x[2] == '1'):\n",
    "        return 'Best customers'\n",
    "    elif (x[0] == '3') & (x[1] == '1') & (x[2] == '1'):\n",
    "        return 'Almost lost'\n",
    "    elif (x[0] == '4') & (x[1] == '1') & (x[2] == '1'):\n",
    "        return 'Lost'\n",
    "    elif (x[0] == '4') & (x[1] == '4') & (x[2] == '4'):\n",
    "        return 'Lost cheap customers'\n",
    "    elif (x[1] == '1'):\n",
    "        return 'Loyal customer'\n",
    "    elif (x[2] == '1'):\n",
    "        return 'Big spenders'\n",
    "    else:\n",
    "        return 'No specific segment'\n"
   ]
  },
